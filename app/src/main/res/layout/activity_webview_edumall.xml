<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <WebView
        android:id="@+id/webview_edumall"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </WebView>

    <ImageView
        android:id="@+id/btn_back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="@dimen/dp3"
        android:padding="@dimen/dp1"
        android:src="@drawable/ic_back_menu"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btn_home"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="@dimen/dp3"
        android:padding="@dimen/dp1"
        android:src="@drawable/icon_home_web"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:layout_margin="@dimen/dp3"
            app:layout_constraintStart_toStartOf="@id/txt_time"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintHeight_percent="0.08"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/txt_time"
            android:id="@+id/img_time"
            android:padding="@dimen/dp3"
            android:src="@drawable/icon_game_time"
            android:layout_width="0dp"
            android:layout_height="0dp"/>
        <TextView

            app:layout_constraintTop_toBottomOf="@id/img_time"
            app:layout_constraintEnd_toStartOf="@id/btn_home"
            android:id="@+id/txt_time"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/txt_name_home"
            android:text="30:00"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


</androidx.constraintlayout.widget.ConstraintLayout>